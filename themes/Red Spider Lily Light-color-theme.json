  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment.block",
        "comment.block.documentation",
        "comment.line"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#414141"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable"],
      "settings": {
        "foreground": "#242421"
      }
    },
    {
      "name": "Variables as parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "This, self, null keywords",
      "scope": [
        "variable.language.this",
        "variable.language.special.self",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword unit",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#5c5151"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call.generic"
      ],
      "settings": {
        "foreground": "#3d352d",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#4d4343"
      }
    },
    {
      "name": "Storage type(let, const)",
      "scope": "storage.type",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage modifier(public)",
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Property  value",
      "scope": "support.constant.property-value",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Font names",
      "scope": "support.constant.font-name",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword control",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword other",
      "scope": "keyword.other",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword (global)",
      "scope": "keyword",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword type",
      "scope": "keyword.type",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    // Markdown
    {
      "name": "Sections",
      "scope": [
        "entity.name.section",
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Italic",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Bold",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strikethrough",
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Quote",
      "scope": "markup.quote",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#442626",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Json string left part",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#242421"
      }
    },
    {
      "name": "Keyword operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": ""
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Other entities",
      "scope": "entity.other",
      "settings": {
        "foreground": "#3d3630"
      }
    },
    {
      "name": "Attribute name",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#e11b32"
      }
    }
  ]
}

  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment.block",
        "comment.block.documentation",
        "comment.line"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#484848"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable"],
      "settings": {
        "foreground": "#f0f0d8"
      }
    },
    {
      "name": "Variables as parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "This keyword",
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword unit",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#c0a8a8"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#d8c0a8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#c0a8a8"
      }
    },
    {
      "name": "Storage type(let, const)",
      "scope": "storage.type",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Property  value",
      "scope": "support.constant.property-value",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Font names",
      "scope": "support.constant.font-name",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword control",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#906060",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Json string left part",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#f0f0d8"
      }
    },
    {
      "name": "Assignment",
      "scope": "keyword.operator.assignment",
      "settings": {
        "foreground": "#f0f0d8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#e11b32"
      }
    },
    {
      "name": "Other entities",
      "scope": "entity.other",
      "settings": {
        "foreground": "#d8c0a8"
      }
    },
    {
      "name": "Attribute name",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#e11b32"
      }
    }
  ]
}

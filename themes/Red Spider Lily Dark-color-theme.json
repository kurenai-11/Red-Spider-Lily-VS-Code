  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment.block",
        "comment.block.documentation",
        "comment.line"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#484848"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable"],
      "settings": {
        "foreground": "#f0f0d8"
      }
    },
    {
      "name": "Variables in prisma",
      "scope": ["variable.language.relations.prisma"],
      "settings": {
        "foreground": "#C0A8A8"
      }
    },
    {
      "name": "Variables as parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "This, self, null keywords",
      "scope": [
        "variable.language.this",
        "variable.language.special.self",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword unit",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#c0a8a8"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call.generic"
      ],
      "settings": {
        "foreground": "#d8c0a8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#c0a8a8"
      }
    },
    {
      "name": "Storage type(let, const)",
      "scope": "storage.type",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage modifier(public)",
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Property  value",
      "scope": "support.constant.property-value",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Escape sequence",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#c0a8a8"
      }
    },
    {
      "name": "Classes",
      "scope": ["support.class", "entity.name.type.class"],
      "settings": {
        "foreground": "#c0a8a8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Components (react etc)",
      "scope": "support.class.component",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Template and interpolated expressions",
      "scope": ["meta.template.expression", "string.interpolated.expression"],
      "settings": {
        "foreground": "#f0f0d8",
        "fontStyle": ""
      }
    },
    {
      "name": "Constant other placeholder",
      "scope": "constant.other.placeholder",
      "settings": {
        "foreground": "#e11b32"
      }
    },
    {
      "name": "Constant character entity (escape character)",
      "scope": "constant.character.entity",
      "settings": {
        "foreground": "#e11b32"
      }
    },
    {
      "name": "Font names",
      "scope": "support.constant.font-name",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword control",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword other",
      "scope": "keyword.other",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword (global)",
      "scope": "keyword",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword type",
      "scope": "keyword.type",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    // Markdown
    {
      "name": "Sections",
      "scope": [
        "entity.name.section",
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Italic",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Bold",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strikethrough",
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Quote",
      "scope": "markup.quote",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#906060",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Json string left part",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#f0f0d8"
      }
    },
    {
      "name": "Keyword operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": ""
      }
    },
    {
      "name": "Key-value separator",
      "scope": [
        "punctuation.separator.key-value",
        "punctuation.separator.dictionary.key-value"
      ],
      "settings": {
        "foreground": "#e11b32"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#e11b32",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Other entities",
      "scope": "entity.other",
      "settings": {
        "foreground": "#d8c0a8"
      }
    },
    {
      "name": "Attribute name",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#e11b32"
      }
    }
  ]
}

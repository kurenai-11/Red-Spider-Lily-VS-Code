  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment.block",
        "comment.block.documentation",
        "comment.line"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#484848"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable"],
      "settings": {
        "foreground": "#eeeeee"
      }
    },
    {
      "name": "Variables in prisma",
      "scope": ["variable.language.relations.prisma"],
      "settings": {
        "foreground": "#c2c2c2"
      }
    },
    {
      "name": "Variables as parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#dfdfdf",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "This, self, null keywords",
      "scope": [
        "variable.language.this",
        "variable.language.self",
        "variable.language.special.self",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#d6d6d6",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Keyword unit",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#bebebe"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call.generic"
      ],
      "settings": {
        "foreground": "#dadada",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#c4c4c4"
      }
    },
    {
      "name": "Storage type(let, const)",
      "scope": "storage.type",
      "settings": {
        "foreground": "#dbdbdb",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Storage modifier(public)",
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#d8d8d8",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Property  value",
      "scope": "support.constant.property-value",
      "settings": {
        "foreground": "#d3d3d3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Escape sequence",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#bbbbbb"
      }
    },
    {
      "name": "Classes",
      "scope": ["support.class", "entity.name.type.class"],
      "settings": {
        "foreground": "#bebebe",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Components (react etc)",
      "scope": "support.class.component",
      "settings": {
        "foreground": "#d8d8d8",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Template and interpolated expressions",
      "scope": ["meta.template.expression", "string.interpolated.expression"],
      "settings": {
        "foreground": "#eeeeee",
        "fontStyle": ""
      }
    },
    {
      "name": "Constant other placeholder",
      "scope": "constant.other.placeholder",
      "settings": {
        "foreground": "#e0e0e0"
      }
    },
    {
      "name": "Constant character entity (escape character)",
      "scope": "constant.character.entity",
      "settings": {
        "foreground": "#dadada"
      }
    },
    {
      "name": "Font names",
      "scope": "support.constant.font-name",
      "settings": {
        "foreground": "#d8d8d8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword control",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#dadada",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Keyword other",
      "scope": "keyword.other",
      "settings": {
        "foreground": "#dfdfdf",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Keyword (global)",
      "scope": "keyword",
      "settings": {
        "foreground": "#dadada",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Keyword type",
      "scope": "keyword.type",
      "settings": {
        "foreground": "#dbdbdb",
        "fontStyle": "italic bold"
      }
    },
    // Markdown
    {
      "name": "Sections",
      "scope": [
        "entity.name.section",
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#d8d8d8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Italic",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Bold",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strikethrough",
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Quote",
      "scope": "markup.quote",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#8b8b8b",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Json string left part",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#ebebeb"
      }
    },
    {
      "name": "Keyword operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#dddddd",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword operator new, of",
      "scope": ["keyword.operator.new", "keyword.operator.expression"],
      "settings": {
        "foreground": "#dddddd",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Key-value separator",
      "scope": [
        "punctuation.separator.key-value",
        "punctuation.separator.dictionary.key-value"
      ],
      "settings": {
        "foreground": "#d8d8d8"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#dbdbdb",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Other entities",
      "scope": "entity.other",
      "settings": {
        "foreground": "#d6d6d6"
      }
    },
    {
      "name": "Attribute name",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#dbdbdb",
        "fontStyle": "bold"
      }
    }
  ]
}

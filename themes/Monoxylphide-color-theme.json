  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment.block",
        "comment.block.documentation",
        "comment.line"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#414141"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable"],
      "settings": {
        "foreground": "#242424"
      }
    },
    {
      "name": "Variables in prisma",
      "scope": ["variable.language.relations.prisma"],
      "settings": {
        "foreground": "#474747"
      }
    },
    {
      "name": "Variables as parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#242421",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "This, self, null keywords",
      "scope": [
        "variable.language.this",
        "variable.language.self",
        "variable.language.special.self",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#242421",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Keyword unit",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#575757"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call.generic"
      ],
      "settings": {
        "foreground": "#3a3a3a",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#4b4b4b"
      }
    },
    {
      "name": "Storage type(let, const)",
      "scope": "storage.type",
      "settings": {
        "foreground": "#242421",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Storage modifier(public)",
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#242421",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Property  value",
      "scope": "support.constant.property-value",
      "settings": {
        "foreground": "#242421",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Escape sequence",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#8f8f8f"
      }
    },
    {
      "name": "Classes",
      "scope": ["support.class", "entity.name.type.class"],
      "settings": {
        "foreground": "#505050",
        "fontStyle": ""
      }
    },
    {
      "name": "Components (react etc)",
      "scope": "support.class.component",
      "settings": {
        "foreground": "#242421",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Template and interpolated expressions",
      "scope": ["meta.template.expression", "string.interpolated.expression"],
      "settings": {
        "foreground": "#242421",
        "fontStyle": ""
      }
    },
    {
      "name": "Constant other placeholder",
      "scope": "constant.other.placeholder",
      "settings": {
        "foreground": "#242421"
      }
    },
    {
      "name": "Constant character entity (escape character)",
      "scope": "constant.character.entity",
      "settings": {
        "foreground": "#242421"
      }
    },
    {
      "name": "Font names",
      "scope": "support.constant.font-name",
      "settings": {
        "foreground": "#242421",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword control",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#242421",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Keyword other",
      "scope": "keyword.other",
      "settings": {
        "foreground": "#242421",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Keyword (global)",
      "scope": "keyword",
      "settings": {
        "foreground": "#242421",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Keyword type",
      "scope": "keyword.type",
      "settings": {
        "foreground": "#242421",
        "fontStyle": "italic bold"
      }
    },
    // Markdown
    {
      "name": "Sections",
      "scope": [
        "entity.name.section",
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#242421",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Italic",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Bold",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strikethrough",
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Quote",
      "scope": "markup.quote",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#242421",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Json string left part",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#242421"
      }
    },
    {
      "name": "Keyword operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#242421",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword operator new, of",
      "scope": ["keyword.operator.new", "keyword.operator.expression"],
      "settings": {
        "foreground": "#242421",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Key-value separator",
      "scope": [
        "punctuation.separator.key-value",
        "punctuation.separator.dictionary.key-value"
      ],
      "settings": {
        "foreground": "#242421"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#242421",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Other entities",
      "scope": "entity.other",
      "settings": {
        "foreground": "#363636"
      }
    },
    {
      "name": "Attribute name",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#242421",
        "fontStyle": "bold"
      }
    }
  ]
}
